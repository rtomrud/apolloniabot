    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["ES2021"],
    "module": "Node16",
    "moduleResolution": "Node16",
    "outDir": "dist",
    "rootDir": "src",
    "skipLibCheck": true,
    "strict": true,
    "target": "ES2021",
  },
  "include": ["src"]
}

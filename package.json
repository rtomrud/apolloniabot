{
  "name": "apolloniabot",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "esbuild --platform=node --packages=external --outdir=dist \"src/**/*.ts\"",
    "format": "prettier --check .",
    "lint": "eslint .",
    "start": "node --watch --experimental-modules --import tsx/esm src/index.ts",
    "test": "node --test --experimental-test-coverage --import tsx/esm **/*.test.ts",
    "posttest": "npm run lint && npm run format"
  },
  "dependencies": {
    "@discordjs/voice": "0.18.0",
    "@distube/ytsr": "2.0.4",
    "discord.js": "14.18.0",
    "distube": "5.0.7",
    "dotenv": "16.5.0",
    "libsodium-wrappers": "0.7.15",
    "opusscript": "0.1.1",
    "spotify-uri": "4.1.0",
    "spotify-url-info": "3.2.18",
    "undici": "7.10.0"
  },
  "devDependencies": {
    "@eslint/js": "9.29.0",
    "esbuild": "0.25.5",
    "eslint": "9.29.0",
    "prettier": "3.5.3",
    "tsx": "4.20.3",
    "typescript": "5.8.3",
    "typescript-eslint": "8.34.0"
  },
  "private": true,
  "type": "module"
}

{
  "name": "lenabot",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "scripts": {
    "lint": "eslint .",
    "prepare": "husky install",
    "start": "pm2 start src/index.js -o out.log -e err.log --time",
    "stop": "pm2 kill",
    "test": "nyc --reporter html tape \"test/**/*.js\" && nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "posttest": "npm run lint",
    "watch": "pm2 start src/index.js --watch"
  },
  "dependencies": {
    "@discordjs/opus": "0.5.3",
    "@discordjs/voice": "0.5.5",
    "bufferutil": "4.0.3",
    "discord.js": "13.0.0-dev.t1628078860.90c2e07",
    "distube": "3.0.0-alpha.18",
    "dotenv": "10.0.0",
    "erlpack": "0.1.3",
    "sodium": "3.0.2",
    "spotify-uri": "2.2.0",
    "spotify-url-info": "2.2.3",
    "utf-8-validate": "5.0.5"
  },
  "devDependencies": {
    "eslint": "7.32.0",
    "eslint-config-pretty-strict": "8.0.1",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "7.0.1",
    "nyc": "15.1.0",
    "pm2": "5.1.0",
    "prettier": "2.3.2",
    "tape": "5.3.0"
  },
  "engines": {
    "node": ">=14"
  },
  "private": true,
  "type": "module"
}

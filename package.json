{
  "name": "apolloniabot",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "esbuild src/index.ts --bundle --outdir=dist --format=esm --platform=node --packages=external",
    "lint": "eslint .",
    "start": "nodemon --exec \"npm run build && node dist/index.js\" --ext ts --watch src",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js",
    "posttest": "npm run lint"
  },
  "dependencies": {
    "@discordjs/opus": "0.9.0",
    "@discordjs/voice": "0.16.1",
    "discord.js": "14.14.1",
    "distube": "4.1.1",
    "dotenv": "16.4.5",
    "sodium-native": "4.1.1",
    "spotify-url-info": "3.2.13",
    "undici": "6.13.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "7.7.0",
    "@typescript-eslint/parser": "7.7.0",
    "esbuild": "0.20.2",
    "esbuild-jest": "0.5.0",
    "eslint": "8.57.0",
    "eslint-config-pretty-strict": "8.1.2",
    "eslint-plugin-jest": "28.2.0",
    "eslint-plugin-prettier": "5.1.3",
    "jest": "29.7.0",
    "nodemon": "3.1.0",
    "prettier": "3.2.5",
    "typescript": "5.4.5"
  },
  "private": true,
  "type": "module"
}
